<generatorConfiguration>
	<!-- 制定mysql的驱动包的路径 千万别放中文路径下 -->
	<classPathEntry location="D:\Java\repository\mysql\mysql-connector-java\5.1.8\mysql-connector-java-5.1.8.jar" />
	<!-- 配置数据源和生成的代码所存放的位置 -->
	<context id="context1">
		<commentGenerator>
			<!-- 去除自动生成的注释 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://182.254.139.119:3306/cost" userId="admin" password="Password01!" />
		<!-- 所生成的实体类的位置默认资源包src -->
		<javaModelGenerator targetPackage="com.cat.autoBean" targetProject="cat_ars Maven Webapp" />
		<!-- 所生成的sqlMap的影射文件的位置,默认资源包src -->
		<sqlMapGenerator targetPackage="com.cat.autoBean" targetProject="cat_ars Maven Webapp" />
		<!-- 为哪些表生成代码 tableName:表名 schema:不用填写,其余属性是禁用例子查询的生成 -->
		<table schema="" tableName="NUSGOProjectInfo" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
		
	
		
	</context>
</generatorConfiguration>
